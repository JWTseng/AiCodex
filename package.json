{
  "name": "tetris-world",
  "version": "3.0.0",
  "type": "module",
  "description": "Classic NES Tetris - Web Implementation",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext ts,js --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,js --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^8.57.0",
    "happy-dom": "^18.0.1",
    "prettier": "^3.3.0",
    "typescript": "^5.3.0",
    "vite": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JWTseng/AiCodex.git"
  },
  "keywords": [
    "tetris",
    "game",
    "html5",
    "typescript",
    "vite"
  ],
  "author": "JWTseng",
  "license": "MIT"
}
